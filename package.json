{
  "name": "api-express",
  "version": "1.0.0",
  "description": "ArtDID Registry API for Alibaba Function Compute",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "NODE_ENV=development ts-node src/index.ts",
    "start": "node -r dotenv/config dist/index.js",
    "docker:build": "docker build -t artdid-api .",
    "docker:run": "docker run -p 9000:9000 -e PRIVATE_KEY=xxx -e CONTRACT_ADDRESS=xxx artdid-api",
    "deploy": "s deploy"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "ethers": "^6.9.0",
    "express": "^4.18.2",
    "form-data": "^4.0.0",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "@serverless-devs/s": "^2.1.10",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/node-fetch": "^2.6.9",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0"
  }
}
